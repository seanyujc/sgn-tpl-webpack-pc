{
  "compilerOptions": {
      "module": "commonjs",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "target": "es5",
      "outDir": "./dist",
      "typeRoots": [
          "node_modules/@types/"
      ],
      "types": [
          "node",
          "angular",
          "angular-animate",
          "angular-ui-router",
          "angular-ui-bootstrap",
          "webpack-env",
          "oclazyload"
      ],
      "sourceMap": true
  },
  "include": [
      "src/**/*"
  ],
  "exclude": [
      "node_modules"
  ]
}