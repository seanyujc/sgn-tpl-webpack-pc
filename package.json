  "scripts": {
    "clean": "rimraf dist && mkdirp dist ./dist/assets ./dist/dll",
    "cpdll": "ncp ./dll/angular-dll.js ./dist/dll/angular-dll.js && ncp ./dll/angular-dll.js.map ./dist/dll/angular-dll.js.map && ncp ./dll/plugins-dll.js ./dist/dll/plugins-dll.js && ncp ./dll/plugins-dll.js.map ./dist/dll/plugins-dll.js.map && ncp ./dll/bootstrap.css ./dist/bootstrap.css && ncp ./dll/assets/ ./dist/assets/",
    "cpassets": "ncp ./src/assets/images ./dist/assets/images",
    "dev": "node ./build/dev-server.js",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=PRO node ./build/build.js",
    "postbuild": "npm run cpdll && npm run cpassets",
    "prebuild:test": "npm run prebuild",
    "build:test": "cross-env NODE_ENV=TEST node ./build/build.js",
    "prebuild:dll": "rimraf dll",
    "build:dll": "webpack -p --config=webpack.dll.js",
    "postbuild:dll": "npm run cpdll",
    "preinstall": "npm i cross-env ncp rimraf mkdirp -g && npm run clean",
    "postinstall": "npm run build:dll && npm run cpassets",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
